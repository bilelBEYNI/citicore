<form method="post" action="{{ path('app_feedback_delete', {'id_FeedBack': feedback.id_FeedBack}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.id_FeedBack) }}">
    <button class="btn">Delete</button>
</form>
