{% extends 'base.html.twig' %}

{% block title %}Feedback{% endblock %}

{% block body %}
    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id_FeedBack</th>
                <td>{{ feedback.idFeedBack }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ feedback.contenu }}</td>
            </tr>
            <tr>
                <th>Date_creation</th>
                <td>{{ feedback.dateCreation ? feedback.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_feedback_index') }}">back to list</a>

    <a href="{{ path('app_feedback_edit', {'id_FeedBack': feedback.id_FeedBack}) }}">edit</a>

    {{ include('feedback/_delete_form.html.twig') }}
{% endblock %}
