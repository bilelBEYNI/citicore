{% extends '/front/index.html.twig' %}
{% block title %}Association{% endblock %}

{% block body %}
{% include 'front/navbar.html.twig' %}
    <h1>Association</h1>

    <table class="table">
        <tbody>
            
            <tr>
                <th>Nom</th>
                <td>{{ association.Nom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ association.Email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ association.Telephone }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ association.Description }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ association.Adresse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_association_index') }}">back to list</a>

    <a href="{{ path('app_association_edit', {'id_association': association.id}) }}">edit</a>

    {{ include('association/_delete_form.html.twig') }}
